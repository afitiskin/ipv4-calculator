{
  "name": "ipv4-calculator",
  "version": "1.0.0",
  "description": "Calculator for IPv4 networks",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint ./src/**/*.js",
    "pretest": "npm run build",
    "test": "mocha --compilers js:babel-register test/*",
