  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-eslint": "^7.0.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.16.0",
    "chai": "^3.5.0",
    "eslint": "^3.6.1",
    "eslint-plugin-babel": "^3.3.0",
    "mocha": "^3.1.0",
    "sinon": "^1.17.6"
  },
  "dependencies": {
    "left-pad": "^1.1.2"
